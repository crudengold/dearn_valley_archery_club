<div id="honours-board-page-body" class="page-body row" data-controller="honours-board-tabs">
  <div class="nav flex-column nav-pills col-3" id="v-pills-tab" role="tablist" aria-orientation="vertical">
    <button class="nav-link active" id="v-pills-international-tab" data-bs-toggle="pill" data-bs-target="#v-pills-international" type="button" role="tab" aria-controls="v-pills-international" aria-selected="true">International/Team GB</button>
    <button class="nav-link" id="v-pills-british-tab" data-bs-toggle="pill" data-bs-target="#v-pills-british" type="button" role="tab" aria-controls="v-pills-british" aria-selected="false">British</button>
    <button class="nav-link" id="v-pills-county-tab" data-bs-toggle="pill" data-bs-target="#v-pills-county" type="button" role="tab" aria-controls="v-pills-county" aria-selected="false">County</button>
    <button class="nav-link" id="v-pills-county-team-tab" data-bs-toggle="pill" data-bs-target="#v-pills-county-team" type="button" role="tab" aria-controls="v-pills-county-team" aria-selected="false">County Team</button>
    <button class="nav-link" id="v-pills-english-tab" data-bs-toggle="pill" data-bs-target="#v-pills-english" type="button" role="tab" aria-controls="v-pills-english" aria-selected="false">English</button>
    <button class="nav-link" id="v-pills-northern-counties-tab" data-bs-toggle="pill" data-bs-target="#v-pills-northern-counties" type="button" role="tab" aria-controls="v-pills-northern-counties" aria-selected="false">Northern Counties</button>
  </div>
  <div class="tab-content col-9" id="v-pills-tabContent">
    <div class="tab-pane fade show active" id="v-pills-international" role="tabpanel" aria-labelledby="v-pills-international-tab" tabindex="0"><%= render 'pages/honours_board/team_gb' %></div>
    <div class="tab-pane fade" id="v-pills-british" role="tabpanel" aria-labelledby="v-pills-british-tab" tabindex="0"><%= render 'pages/honours_board/british' %></div>
    <div class="tab-pane fade" id="v-pills-county" role="tabpanel" aria-labelledby="v-pills-county-tab" tabindex="0"><%= render 'pages/honours_board/county' %></div>
    <div class="tab-pane fade" id="v-pills-county-team" role="tabpanel" aria-labelledby="v-pills-county-team-tab" tabindex="0"><%= render 'pages/honours_board/county_team' %></div>
    <div class="tab-pane fade" id="v-pills-english" role="tabpanel" aria-labelledby="v-pills-english-tab" tabindex="0"><%= render 'pages/honours_board/english' %></div>
    <div class="tab-pane fade" id="v-pills-northern-counties" role="tabpanel" aria-labelledby="v-pills-northern-counties-tab" tabindex="0"><%= render 'pages/honours_board/northern_counties' %></div>
  </div>

</div>
